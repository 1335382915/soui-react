{
  "name": "@sodalife/soui-react",
  "version": "0.3.5",
  "description": "soda ui components library, for react",
  "main": "dist/soui.js",
  "browser": "dist/soui.standalone.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:sodalife/soui-react.git"
  },
  "author": "yelo",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf ./dist ./storybook-static ./docs/storybook",
    "build": "npm run build:umd && npm run build:standalone",
    "build:umd": "rollup -c",
    "build:standalone": "webpack --config webpack.conf.prod.js",
    "storybook": "start-storybook -p 9001 -c storybook",
    "build-storybook": "npm run build:umd && build-storybook -c storybook -o ./docs/storybook",
    "gh-pages": "npm run build-storybook && gh-pages -d docs --dotfiles",
    "prepublish": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "keymirror": "^0.1.1",
    "lodash-es": "^4.17.4",
    "prop-types": "^15.5.10",
    "rc-animate": "^2.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "autoprefixer": "^7.1.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-postcss": "^0.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.0.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "lost": "^8.2.0",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules": "^0.8.0",
    "precss": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-postcss": "^0.5.5",
    "style-loader": "^0.18.2",
    "unminified-webpack-plugin": "^1.2.0",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  }
}
